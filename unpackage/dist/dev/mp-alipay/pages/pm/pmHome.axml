<view><view class="pm-home"><view class="header"><block a:if="{{userToken!==''}}"><view class="person-box"><image class="avatar" src="{{userAvatar||'/static/images/pm/av.png'}}" background-size="cover"></image><view class="info-box"><view class="userName"><view><view class="name">{{username}}</view><view class="phone">{{userPhone}}</view></view><block a:if="{{isAuth*1}}"><view data-event-opts="{{[['tap',[['authAction',['$event']]]]]}}" class="auth-btn" catchTap="__e">已认证</view></block><block a:if="{{!isAuth*1}}"><view data-event-opts="{{[['tap',[['authAction',['$event']]]]]}}" class="auth-btn" catchTap="__e">未认证</view></block></view></view></view></block><block a:if="{{!userToken}}"><view class="person-box"><image class="avatar" src="/static/images/pm/av.png"></image><view class="info-box" style="margin-top:40rpx;"><view class="userName"><view class="name">未登入</view></view></view></view></block></view><view class="userinfo-bg-view"><view class="home-userinfo-view"><view a:for="{{items}}" a:for-item="item" a:for-index="index" a:key="index"><block a:if="{{item.hasAuth}}"><view class="user-content-cell" data-index="{{index}}" data-event-opts="{{[['tap',[['tapItem',['$event']]]]]}}" catchTap="__e"><image class="item-icon" src="{{item.icon}}" mode="aspectFit"></image><view class="user-content-cell-text global-title-color">{{item.name}}</view><view class="one one-apparrow-right"></view></view></block></view></view></view></view><slider-picker vue-id="798cbbd7-1" sliderList="{{sliderList}}" ref="__r" data-ref="sliderPicker" data-event-opts="{{[['^selectWay',[['handleSliderOperateFun']]]]}}" onSelectWay="__e" onVueInit="__l"></slider-picker><block a:if="{{!userToken}}"><button data-event-opts="{{[['tap',[['login',['$event']]]]]}}" class="login-btn" catchTap="__e">立即登录</button></block><wechat-user-info vue-id="798cbbd7-2" show="{{wechatUserInfoShow}}" data-event-opts="{{[['^getWechatUserInfo',[['getWechatUserInfo']]]]}}" onGetWechatUserInfo="__e" onVueInit="__l"></wechat-user-info></view>