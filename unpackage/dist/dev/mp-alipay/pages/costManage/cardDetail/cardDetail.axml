<view class="card-detail-page"><block a:if="{{detailData!=null&&detailData.length>0}}"><view><view class="list-item" a:for="{{detailData}}" a:for-item="item" a:for-index="index" a:key="index"><view class="top"><view class="name">{{item.consumptionTypeText}}</view><view class="money">{{"- "+item.consumptionMoney}}</view></view><view class="bot"><view class="time">{{"支出时间："+item.consumptionTime}}</view><view class="type">{{item.deductionName}}</view></view></view></view></block><block a:if="{{detailData==null||detailData.length==0}}"><view class="empty">暂无消费记录</view></block></view>