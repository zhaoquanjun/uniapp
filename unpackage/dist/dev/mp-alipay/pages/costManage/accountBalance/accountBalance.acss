view {
  box-sizing: border-box;
}
.scroll-area {
  width: 100%;
  height: 100%;
}
.loading {
  width: 100%;
  height: 0;
  line-height: 100rpx;
  text-align: center;
  font-size: 24rpx;
  color: #999;
  opacity: 0;
  background-color: #f5f5f5;
  -webkit-transition: all .25s ease-in-out;
  transition: all .25s ease-in-out;
}
.show {
  height: 100rpx;
  opacity: 1;
}
.balance-list--page {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
	background-color: #f7f8f9;
}
.header-wrapper {
	width: 100%;
	background-color: #fff;
}
.balance-list--page .header-tab {
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin: 0 40rpx;
  width: calc(100% - 80rpx);
  height: 96rpx;
  border-bottom: 2rpx solid #F5F5F5;
}
.balance-list--page .header-tab .tab-item {
  padding-top: 16rpx;
  width: 50%;
  font-size: 28rpx;
  color: #585858;
  text-align: center;
}
.balance-list--page .header-tab .tab-item.active {
  font-weight: 500;
  color: #333;
}
.balance-list--page .header-tab .active-bg {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 33%;
  height: 6rpx;
  -webkit-transition: all .2s ease-in-out;
  transition: all .2s ease-in-out;
}
.balance-list--page .header-tab .active-bg.second {
  left: 33%;
}
.balance-list--page .header-tab .active-bg.third {
  left: 66%;
}
.balance-list--page .header-tab .active-bg::after {
  position: absolute;
  bottom: 0;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  display: block;
  content: '';
  width: 120rpx;
  height: 6rpx;
  background: -webkit-linear-gradient(right, rgba(247, 165, 0, 1) 0%, rgba(237, 108, 0, 1) 100%);
  background: linear-gradient(270deg, rgba(247, 165, 0, 1) 0%, rgba(237, 108, 0, 1) 100%);
  border-radius: 4rpx;
}
.balance-list--page .content {
  position: absolute;
  top: 104rpx;
  left: 0;
  width: 100%;
  height: calc(100% - 270rpx);
  background-color: #f7f8f9;
}
.balance-list--page .content .balance-list {
  padding: 0 48rpx;
	background-color: #fff;
}
.balance-list--page .content .balance-list .list-item {
  padding: 30rpx 0 32rpx;
  border-bottom: 2rpx solid #F5F5F5;
}
.balance-list--page .content .balance-list .top {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.balance-list--page .content .balance-list .top .name {
  font-size: 32rpx;
  line-height: 48rpx;
  color: #333;
}
.balance-list--page .content .balance-list .top .money {
  font-size: 28rpx;
  font-weight: 500;
  color: #333;
  line-height: 40rpx;
}
.balance-list--page .content .balance-list .bot {
  padding-top: 14rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 24rpx;
  line-height: 40rpx;
  color: #666;
}
.balance-list--page .content .balance-list .bot .type {
  color: #999;
}
.balance-list--page .btn-groups {
  position: absolute;
  bottom: 30rpx;
  left: 0;
  padding: 0 36rpx;
  width: 100%;
  height: 100rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.balance-list--page .btn-groups .btn {
  width: 50%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 32rpx;
  color: #333;
  border-radius: 8rpx;
}
.balance-list--page .btn-groups .btn.send {
  color: #fff;
  font-weight: 500;
  background-color: #464646;
}

