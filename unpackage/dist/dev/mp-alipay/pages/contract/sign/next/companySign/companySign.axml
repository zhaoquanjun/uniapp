<view class="pageContent"><view class="item"><view class="item-title">输入信息</view><view class="item-Content"><view class="message"><text>公司</text><input type="text" placeholder="请输入" data-field="companyName" data-event-opts="{{[['input',[['bindKeyInput',['$event']]]],['blur',[['handleBlurFun',['$event']]]]]}}" value="{{companyName}}" onInput="__e" onBlur="__e"/></view><view class="{{('search-result--list '+(searchCompanyResultList.length>0?'show':''))}}"><view class="list-item" data-name="{{item.name}}" data-event-opts="{{[['tap',[['handleSelectCompanyDataFun',['$event']]]]]}}" onTap="__e" a:for="{{searchCompanyResultList}}" a:for-item="item" a:for-index="index" a:key="index">{{item.name}}</view></view></view><view class="item-Content"><view class="message"><text>经办人姓名</text><input type="text" placeholder="请输入" data-field="userName" data-event-opts="{{[['input',[['bindKeyInput',['$event']]]],['blur',[['handleBlurFun',['$event']]]]]}}" value="{{userName}}" onInput="__e" onBlur="__e"/></view><view class="{{('search-result--list '+(searchPersonResultList.length>0?'show':''))}}"><view class="list-item" data-name="{{item.name}}" data-phone="{{item.phone}}" data-event-opts="{{[['tap',[['handleSelectPersonDataFun',['$event']]]]]}}" onTap="__e" a:for="{{searchPersonResultList}}" a:for-item="item" a:for-index="index" a:key="index">{{item.name+" - "+item.phone}}</view></view></view><view class="item-Content"><view class="message"><text>经办人手机号</text><input type="text" maxlength="11" placeholder="请输入手机号" data-field="userPhone" data-event-opts="{{[['input',[['bindKeyInput',['$event']]]]]}}" value="{{userPhone}}" onInput="__e"/></view></view></view><view class="item"><view class="item-title">签署要求</view><view class="item-Content"><view data-event-opts="{{[['tap',[['checkedTap1',['$event']]]]]}}" class="othermessage" catchTap="__e"><radio value="r1" checked="{{checked1}}"></radio><view style="margin-left:8rpx;padding-top:8rpx;">企业章</view></view></view><view class="item-Content"><view data-event-opts="{{[['tap',[['checkedTap2',['$event']]]]]}}" class="othermessage" catchTap="__e"><radio value="r2" checked="{{checked2}}"></radio><view style="margin-left:8rpx;padding-top:8rpx;">法人代表章</view></view></view></view><view class="bottom-btn-container"><text data-event-opts="{{[['tap',[['back',['$event']]]]]}}" class="back" catchTap="__e">返回</text><block a:if="{{!ishighLight}}"><text class="next" style="background:#999999;">确定</text></block><block a:if="{{ishighLight}}"><text data-event-opts="{{[['tap',[['next',['$event']]]]]}}" class="next" style="background:#464646;" catchTap="__e">确定</text></block></view></view>