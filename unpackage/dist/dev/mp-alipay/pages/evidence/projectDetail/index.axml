<block a:if="{{projectDetail}}"><view class="project-detail--page"><view class="project-info section"><view class="title">{{projectDetail.name}}</view><view class="time-status"><text class="'status ' + (projectDetail.status == 1 ? 'going' : projectDetail.status == 2 ? 'complete' : '')">{{projectDetail.status==1?'进行中':projectDetail.status==2?'已完成':''}}</text><text class="time">{{"创建时间："+(projectDetail.gmtCreate?$root.m0:'')}}</text></view><view class="desc"><block a:if="{{projectDetail.description}}"><view data-event-opts="{{[['tap',[['_handleAddDesc',['$event']]]]]}}" class="words" onTap="__e">{{projectDetail.description}}</view></block><block a:else><view data-event-opts="{{[['tap',[['_handleAddDesc',['$event']]]]]}}" class="empty" onTap="__e">点击添加项目描述<text class="iconfont "></text></view></block></view></view><view data-event-opts="{{[['tap',[['_handleViewMembers',['$event']]]]]}}" class="project-members section" onTap="__e"><text class="text">项目成员</text><text class="iconfont iconright-arrow"></text></view><scroll-view class="evidence-list section" scroll-y="true" scroll-anchoring="true" refresher-enabled="true" data-event-opts="{{[['scrollToLower',[['getMoreDataFun',['$event']]]]]}}" onScrollToLower="__e"><view class="title">证据列表</view><block a:if="{{evidences.length>0}}"><view><view class="item" a:for="{{$root.l0}}" a:for-item="item" a:for-index="index" a:key="index"><view class="content"><view class="title">{{item.$orig.fileName}}</view><view class="time">{{item.$orig.gmtCreate?item.m1:''}}</view></view><view data-event-opts="{{[['tap',[['_handleOperateMore',['$0'],[[['evidences','',index]]]]]]]}}" class="more" onTap="__e"><text class="iconfont iconmore"></text></view></view></view></block><block a:else><view class="ev-empty">暂无关联证据</view></block></scroll-view><slider-picker vue-id="62a9328e-1" sliderList="{{sliders}}" ref="__r" data-ref="sliderPicker" data-event-opts="{{[['^selectWay',[['_handleOperateItem']]]]}}" onSelectWay="__e" onVueInit="__l"></slider-picker></view></block>