<view class="pay-detail--page"><view class="title">合同主题</view><view class="desc">{{contractTitle}}</view><view class="title">填写发票申请</view><view class="info-list"><view class="list-item"><view class="label">抬头类型</view><view class="value" style="width:80%;"><radio-group data-event-opts="{{[['change',[['handleSelectTypeFun',['$event']]]]]}}" onChange="__e"><label style="margin-left:16rpx;" a:for="{{checkList}}" a:for-item="item" a:for-index="index" a:key="index"><radio value="{{item.value}}" checked="{{item.checked}}"></radio><text>{{item.label}}</text></label></radio-group></view></view><view class="list-item"><view class="label">开票方</view><view class="value">{{companyInfo.sellerCompanyName}}</view></view><block a:if="{{checked==2}}"><view class="list-item"><view class="label">发票企业名称</view><view class="value"><input type="text" data-key1="companyInfo" data-key2="buyerName" placeholder="请输入" data-event-opts="{{[['input',[['changeInputValueFun',['$event']]]]]}}" value="{{companyInfo.buyerName}}" onInput="__e"/></view></view></block><block a:if="{{checked==2}}"><view class="list-item"><view class="label">纳税人识别号</view><view class="value"><input type="number" data-key1="companyInfo" data-key2="buyerTaxNo" placeholder="请输入" data-event-opts="{{[['input',[['changeInputValueFun',['$event']]]]]}}" value="{{companyInfo.buyerTaxNo}}" onInput="__e"/></view></view></block><view class="list-item gap"><view class="label">发票类型</view><view data-event-opts="{{[['tap',[['handleSelectInvoiceTypeFun',['$event']]]]]}}" class="{{('value select-area '+(isDown?'down':'up'))}}" onTap="__e">{{''+(invoiceTypeValue.label?invoiceTypeValue.label:'请选择')}}</view></view><block a:if="{{invoiceTypeValue.value=='s'}}"><view><view class="list-item"><view class="label">地址</view><input type="text" data-key1="invoiceValue" data-key2="buyerAddress" placeholder="请输入" data-event-opts="{{[['input',[['changeInputValueFun',['$event']]]]]}}" value="{{invoiceValue.buyerAddress}}" onInput="__e"/></view><view class="list-item"><view class="label">电话</view><input type="number" data-key1="invoiceValue" data-key2="buyerPhone" placeholder="请输入" data-event-opts="{{[['input',[['changeInputValueFun',['$event']]]]]}}" value="{{invoiceValue.buyerPhone}}" onInput="__e"/></view><view class="list-item"><view class="label">开户行</view><input type="text" data-key1="invoiceValue" data-key2="buyerBank" placeholder="请输入" data-event-opts="{{[['input',[['changeInputValueFun',['$event']]]]]}}" value="{{invoiceValue.buyerBank}}" onInput="__e"/></view><view class="list-item"><view class="label">开户账号</view><input type="number" data-key1="invoiceValue" data-key2="buyerBankNo" placeholder="请输入" data-event-opts="{{[['input',[['changeInputValueFun',['$event']]]]]}}" value="{{invoiceValue.buyerBankNo}}" onInput="__e"/></view></view></block><view class="list-item gap"><view class="label">发票推送方式</view><view data-event-opts="{{[['tap',[['handleSelectPostWayFun',['$event']]]]]}}" class="{{('value select-area '+(isDown?'down':'up'))}}" onTap="__e">{{''+(postWayValue.label?postWayValue.label:'请选择')}}</view></view><block a:if="{{postWayValue.value=='1'||postWayValue.value=='2'}}"><view class="list-item"><view class="label">手机号</view><input type="number" data-key1="postvalue" data-key2="pushPhone" placeholder="请输入" data-event-opts="{{[['input',[['changeInputValueFun',['$event']]]]]}}" value="{{postvalue.pushPhone}}" onInput="__e"/></view></block><block a:if="{{postWayValue.value=='0'||postWayValue.value=='2'}}"><view class="list-item"><view class="label">邮箱</view><input type="text" data-key1="postvalue" data-key2="pushEmail" placeholder="请输入" data-event-opts="{{[['input',[['changeInputValueFun',['$event']]]]]}}" value="{{postvalue.pushEmail}}" onInput="__e"/></view></block></view><view class="btn-wrapper"><view data-event-opts="{{[['tap',[['handleConfirmFun',['$event']]]]]}}" class="confirm-btn" onTap="__e">申请开票</view></view><slider-picker vue-id="53c96d58-1" sliderList="{{list}}" ref="__r" data-ref="{{true}}" data-event-opts="{{[['^selectWay',[['handleSelectItemFun']]]]}}" onSelectWay="__e" onVueInit="__l"></slider-picker></view>