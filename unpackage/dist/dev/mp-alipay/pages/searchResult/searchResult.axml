<view><view class="contract-search"><search vue-id="2907bd14-1" timeSearch="{{true}}" :isShowCancel="{{true}}" data-event-opts="{{[['^inputChanged',[['inputSearchFun']]],['^cancelClicked',[['cancleSearchFun']]]]}}" onInputChanged="__e" onCancelClicked="__e" onVueInit="__l"></search></view><block a:if="{{contracts.length>0}}"><view class="recent-search">搜索结果</view></block><view class="contract" style="border-bottom:2rpx solid #F5F5F5FF;" data-item="{{item}}" data-index="{{index}}" data-event-opts="{{[['tap',[['pushToDetail',['$event']]]]]}}" onTap="__e" a:for="{{contracts}}" a:for-item="item" a:for-index="index" a:key="index"><view class="contract-introduce" style="border-bottom:2rpx solid #ffffff;"><view class="contract-title"><search-highlight-text-view vue-id="{{'2907bd14-2-'+index}}" highlight="highlight" text="{{item.contractTitle}}" keyword="{{keyName}}" onVueInit="__l"></search-highlight-text-view></view><view class="contract-right"><view class="status-icon" style="{{('background: '+item.bgStyle+';')}}"></view><view class="contract-state">{{item.statusText}}</view></view></view><view class="contract-related"><view class="contract-sponsor">{{"发起方："+item.initiatorName+''}}</view><view class="contract-signatory">{{"签署方："+item.signNames+''}}</view></view><view class="contract-other"><view class="time">{{item.gmtModified+''}}</view></view></view><block a:if="{{contracts.length<=0}}"><view class="empty-data placeholder-color">暂无相关合同</view></block></view>