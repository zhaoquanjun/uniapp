<view class="{{('slider-picker--area '+(wrapperShow?'wrapper-show':''))}}"><view data-event-opts="{{[['tap',[['hide',['$event']]]]]}}" class="mask" onTap="__e"></view><view class="{{('slider-box '+(contentShow?'content-show':''))}}"><view class="slider-item" data-value="{{item.value}}" data-index="{{index}}" data-event-opts="{{[['tap',[['handleSelectSliderValueFun',['$event']]]]]}}" onTap="__e" a:for="{{sliderList}}" a:for-item="item" a:for-index="index" a:key="index">{{item.label}}</view></view></view>