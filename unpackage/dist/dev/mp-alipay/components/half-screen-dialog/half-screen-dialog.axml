<view class="page"><image class="weui-btn_default--fixed" src="https://shouyiner-prod.oss-cn-beijing.aliyuncs.com/wxapp/shanqian/component/newadd.png" data-event-opts="{{[['tap',[['open',['$event']]]]]}}" catchTap="__e"></image><block a:if="{{wrapperShow}}"><view class="fadeIn"><view data-event-opts="{{[['tap',[['close',['$event']]]]]}}" class="weui-mask" onTap="__e"></view><view class="{{('weui-half-screen-dialog '+(contentShow?'show':''))}}"><view class="weui-half-screen-dialog__bd"><view class="meun-list__item" data-item="{{item}}" data-type="{{item.type}}" data-event-opts="{{[['tap',[['openMenu',['$event']]]]]}}" catchTap="__e" a:for="{{menuList}}" a:for-item="item" a:for-index="index" a:key="index"><image src="{{item.imgUrl}}" mode="aspectFit"></image><text>{{item.name}}</text></view></view><view data-event-opts="{{[['tap',[['close',['$event']]]]]}}" class="weui-half-screen-dialog__hd__side" onTap="__e">取消</view></view></view></block></view>