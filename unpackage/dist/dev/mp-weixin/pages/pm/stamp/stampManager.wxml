ck><block wx:if="{{item.auth==0||item.auth==1}}"><image class="userList-icon" src="https://shouyiner-prod.oss-cn-beijing.aliyuncs.com/wxapp/shanqian/account/person.png"></image></block><view class="userList-name">{{''+(item.companyName?item.companyName:'')+item.name+''}}</view><block wx:if="{{item.authStatus}}"><view class="userList-state" style="{{('background:'+(item.authStatus==1?'rgba(62,180,168,0.1)':item.authStatus==2?'#FDF0E5':'#F4F4F4')+'; color:'+(item.authStatus==1?'#3EB4A8':item.authStatus==2?'#ED6C00':'#919191'))}}">{{''+(item.authStatus=='1'?"已认证":item.authStatus==2?"认证中":"未认证")}}</view></block><block wx:if="{{item.auth==0||item.auth==1}}"><view class="userList-state" style="{{('background:'+(item.auth==1?'rgba(62,180,168,0.1)':'#F4F4F4')+'; color:'+(item.auth==1?'#3EB4A8':'#919191'))}}">{{''+(item.auth=='1'?"已认证":"未认证")}}</view></block></view></block></scroll-view><view class="scroll-under"></view></view></view><slider-picker class="vue-ref" vue-id="2a9545f2-1" sliderList="{{list}}" data-ref="sliderPicker" data-event-opts="{{[['^selectWay',[['handleSelectItemFun']]]]}}" bind:selectWay="__e" bind:__l="__l"></slider-picker></view>