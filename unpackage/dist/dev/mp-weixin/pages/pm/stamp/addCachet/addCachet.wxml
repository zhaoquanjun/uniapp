<view><block wx:for="{{dataMap}}" wx:for-item="data" wx:for-index="index" wx:key="index"><view class="input-view" data-index="{{index}}"><text class="title">{{data.title}}</text><input class="input" data-index="{{index}}" placeholder="{{data.placeholder}}" auto-focus="{{true}}" data-event-opts="{{[['input',[['bindKeyInput',['$event']]]]]}}" bindinput="__e"/></view></block><text class="stamp-title">印章颜色</text><view class="color-view"><block wx:for="{{colors}}" wx:for-item="colorData" wx:for-index="index" wx:key="index"><view class="color" style="{{('border:'+(selectIndex==index?'3rpx '+colorData.rgb+' solid':''))}}" data-index="{{index}}" data-event-opts="{{[['tap',[['chooseColor',['$event']]]]]}}" catchtap="__e"><view class="color-content" style="{{('background-color:'+colorData.rgb)}}"></view></view></block></view><button data-event-opts="{{[['tap',[['createCachet',['$event']]]]]}}" class="submit global-yellow-color" catchtap="__e">创建</button></view>