
page {
  background-color: #F7F8F9;
}
.type-select {
  font-size: 32rpx;
}
.is-nologin, .is-nocontent {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.is-nologin text, .is-nocontent text {
  margin-top: -40rpx;
  font-size: 32rpx;
  color: #666;
}
.type-select-header {
  font-size: 28rpx;
  height: 116rpx;
  line-height: 116rpx;
  padding: 0 50rpx;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
}
.type-select-header .one{
  font-size: 28rpx;
  position: relative;
  top:4rpx
}
.types-select-container{
  width:670rpx;
  margin: 42rpx auto;
  background:rgba(255,255,255,1);
  border-radius:8rpx;
}
.types-select-container .type-item{
  font-size:30rpx;
  font-weight:400;
  color:#333333;
  height: 120rpx;
  line-height: 120rpx;
  margin: 0 32rpx;
  border-bottom: 2rpx solid #F5F5F5;
}
.types-select-container .type-item .one{
  float: right;
  color:#CECECE;
}
.item-count{
  float:right;
  margin-right: 22rpx;
  color: #8B8B8B;
  font-size: 28rpx;
  font-weight:500;
  line-height: 110rpx;
}

