<view class="van-picker custom-class"><block a:if="{{showToolbar}}"><view class="van-picker__toolbar van-hairline--top-bottom toolbar-class"><view class="van-picker__cancel" hover-class="van-picker__cancel--hover" hover-stay-time="70" data-type="cancel" data-event-opts="{{[['tap',[['emit',['$event']]]]]}}" onTap="__e">{{''+cancelButtonText+''}}</view><block a:if="{{title}}"><view class="van-picker__title van-ellipsis">{{title}}</view></block><view class="van-picker__confirm" hover-class="van-picker__confirm--hover" hover-stay-time="70" data-type="confirm" data-event-opts="{{[['tap',[['emit',['$event']]]]]}}" onTap="__e">{{''+confirmButtonText+''}}</view></view></block><block a:if="{{loading}}"><view class="van-picker__loading"><loading vue-id="47b6ec87-1" color="#1989fa" onVueInit="__l"></loading></view></block><view class="van-picker__columns" style="{{('height: '+itemHeight*visibleItemCount+'px')}}" catch:touchmove="noop"><picker-column class="van-picker__column" vue-id="{{'47b6ec87-2-'+index}}" data-index="{{index}}" custom-class="column-class" value-key="{{valueKey}}" initial-options="{{$root.m0?item:item.values}}" default-index="{{item.defaultIndex||defaultIndex}}" item-height="{{itemHeight}}" visible-item-count="{{visibleItemCount}}" active-class="active-class" data-event-opts="{{[['^change',[['onChange']]]]}}" onChange="__e" a:for="{{$root.m1?[columns]:columns}}" a:for-item="item" a:for-index="index" a:key="index" onVueInit="__l"></picker-column><view class="van-picker__frame van-hairline--top-bottom" style="{{('height: '+itemHeight+'px')}}"></view></view></view>