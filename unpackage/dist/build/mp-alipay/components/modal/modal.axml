<block a:if="{{isShow}}"><view class="modal-area"><view class="modal-wrapper"><view class="header">{{title}}</view><view class="content"><scroll-view class="inner" scroll-y="true"><view class="item" a:for="{{content}}" a:for-item="item" a:for-index="index" a:key="index">{{item}}</view></scroll-view></view><view data-event-opts="{{[['tap',[['handleCloseFun',['$event']]]]]}}" class="footer" onTap="__e">知道了</view></view></view></block>