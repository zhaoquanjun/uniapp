.dialog-section .add-btn{width:110rpx;height:110rpx;position:fixed;bottom:100rpx;right:48rpx;border-radius:50%;line-height:110rpx}.dialog-section .add-btn image{width:40rpx;height:40rpx}.dialog-content{position:absolute;left:0;bottom:0;width:100%;border-radius:0;padding:0;-webkit-transform:translateY(100%);transform:translateY(100%);-webkit-transition:-webkit-transform .25s ease-in-out;transition:-webkit-transform .25s ease-in-out;transition:transform .25s ease-in-out;transition:transform .25s ease-in-out,-webkit-transform .25s ease-in-out}.mask{position:absolute;top:0;left:0;width:100%;height:100%;background-color:rgba(0,0,0,.6)}.dialog-content.show{-webkit-transform:translateY(0);transform:translateY(0)}.list-wrapper{width:100%;height:280rpx;padding:0;background:#f7f8f9;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-align:center;-webkit-align-items:center;align-items:center;-webkit-justify-content:space-around;justify-content:space-around}.cancel-btn{left:0;width:100%;height:80rpx;background:#fff;line-height:80rpx;text-align:center;font-size:28rpx}.meun-list__item{text-align:center}.meun-list__item image{width:84rpx;height:84rpx}.meun-list__item text{display:block;height:34rpx;font-size:24rpx;font-weight:400;color:#999;line-height:34rpx;margin-top:34rpx}