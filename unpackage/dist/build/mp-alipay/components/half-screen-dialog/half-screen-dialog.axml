<view class="dialog-section"><image class="add-btn" src="https://shouyiner-prod.oss-cn-beijing.aliyuncs.com/wxapp/shanqian/component/newadd.png" data-event-opts="{{[['tap',[['open',['$event']]]]]}}" catchTap="__e"></image><block a:if="{{wrapperShow}}"><view class="fadeIn"><view data-event-opts="{{[['tap',[['close',['$event']]]]]}}" class="mask" onTap="__e"></view><view class="{{('dialog-content '+(contentShow?'show':''))}}"><view class="list-wrapper"><view class="meun-list__item" data-item="{{item}}" data-type="{{item.type}}" data-event-opts="{{[['tap',[['openMenu',['$event']]]]]}}" catchTap="__e" a:for="{{menuList}}" a:for-item="item" a:for-index="index" a:key="index"><image src="{{item.imgUrl}}" mode="aspectFit"></image><text>{{item.name}}</text></view></view><view data-event-opts="{{[['tap',[['close',['$event']]]]]}}" class="cancel-btn" onTap="__e">取消</view></view></view></block></view>