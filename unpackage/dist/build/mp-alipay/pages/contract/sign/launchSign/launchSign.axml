<view><view class="pageContent"><view class="pageHeader"><view class="theme-title">{{themeTitle}}</view><view class="input-name-box"><textarea name="content" maxlength="100" placeholder="请不要包括特殊字符，且长度不要超过100字" data-event-opts="{{[['input',[['textInput',['$event']]]]]}}" onInput="__e"></textarea></view></view><view class="file-content"><text class="file-title">{{fileTitle}}</text><view data-event-opts="{{[['tap',[['checkFileAction',['$event']]]]]}}" class="file-view" catchTap="__e"><image class="file-image" src="https://shouyiner-prod.oss-cn-beijing.aliyuncs.com/wxapp/shanqian/certificate/file.png"></image><text class="file-name text-apostrophe">{{fileName}}</text><text class="file-format text-apostrophe"></text></view></view><view class="file-signer"><text class="linkman-title">{{linkmanTitle}}</text><view class="linkman-add-sponsor"><text class="sponsor">{{sponsor}}</text></view></view><block a:if="{{ishighLight}}"><button data-event-opts="{{[['tap',[['launchSignAction',['$event']]]]]}}" class="nextButton" style="background-color:#464646;" catchTap="__e">{{signText}}</button></block><block a:if="{{!ishighLight}}"><button class="nextButton" style="background:#B7B7B7;">{{signText}}</button></block></view><view class="{{('user-dialog '+(showDialog?'user-dialog--show':''))}}"><view data-event-opts="{{[['tap',[['toggleDialog',['$event']]]]]}}" class="user-dialog__mask" onTap="__e"></view><view class="user-dialog__container"><view class="userSwitch">切换身份</view><scroll-view style="height:540rpx;" scroll-y="{{true}}" nowrap="{{true}}"><view class="userList" data-item="{{item}}" data-index="{{index}}" data-event-opts="{{[['tap',[['chooseSponsor',['$event']]]]]}}" onTap="__e" a:for="{{companyList}}" a:for-item="item" a:for-index="index" a:key="index"><image class="userList-icon" src="https://shouyiner-prod.oss-cn-beijing.aliyuncs.com/wxapp/shanqian/account/user.png"></image><view class="userList-name">{{''+item.companyName+''}}</view></view></scroll-view><view class="scroll-under"></view></view></view></view>