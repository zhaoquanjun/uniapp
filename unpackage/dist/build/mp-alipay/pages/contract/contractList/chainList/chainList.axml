<view><view class="pl-search"><search vue-id="542fe310-1" onTimeSearch="true" isShowCancel="true" data-event-opts="{{[['^inputChanged',[['inputSearchFun']]],['^cancelClicked',[['cancleSearchFun']]]]}}" onInputChanged="__e" onCancelClicked="__e" onVueInit="__l"></search></view><view class="list-wrapper_title">{{"已选："+selectList.length}}</view><scroll-view class="list-wrapper" scroll-y="true" scroll-anchoring="true" scroll-with-animation="true" data-event-opts="{{[['scrollToLower',[['scrollToBottom',['$event']]]]]}}" onScrollToLower="__e"><checkbox-group data-event-opts="{{[['change',[['getSelectList',['$event']]]]]}}" onChange="__e"><view class="contract" data-index="{{index}}" data-item="{{item}}" a:for="{{contracts}}" a:for-item="item" a:for-index="index" a:key="index"><view class="checkbox-wrap"><checkbox value="{{item.id}}"><view class="checkbox-content"><view class="contract-introduce"><view class="contract-title"><search-highlight-text-view vue-id="{{'542fe310-2-'+index}}" highlight="highlight" text="{{item.name}}" keyword="{{searchParams}}" onVueInit="__l"></search-highlight-text-view></view><view class="contract-right"><view class="{{('status-icon '+(item.status==1?'success':item.status==99?'fail':''))}}"></view><view class="contract-state">{{item.status==0?'存证中...':item.status==1?'存证成功':item.status==99?'存证失败':''}}</view></view></view><view class="contract-related"><view class="contract-sponsor">{{"存证类型："+(item.evidenceType==2?'数据存证':'签署存证')+''}}</view></view><view class="contract-other"><view class="contract-updatetime">存证时间</view><view class="contract-updatetime-middle"></view><view class="time">{{item.gmtModified+''}}</view></view></view></checkbox></view></view></checkbox-group></scroll-view><block a:if="{{contracts.length<=0}}"><view class="empty-data placeholder-color">暂无数据</view></block><button data-event-opts="{{[['tap',[['saveContractEvidenceRel',['$event']]]]]}}" class="launchButton" onTap="__e">确定添加</button></view>