<view><view class="contract-search"><search vue-id="ad0108ae-1" onTimeSearch="true" isShowCancel="true" data-event-opts="{{[['^inputChanged',[['inputSearchFun']]],['^cancelClicked',[['cancleSearchFun']]]]}}" onInputChanged="__e" onCancelClicked="__e" onVueInit="__l"></search></view><view class="contract-template" data-id="{{item.id}}" data-event-opts="{{[['tap',[['templateSelect',['$event']]]]]}}" onTap="__e" a:for="{{contractTemplates}}" a:for-item="item" a:for-index="index" a:key="index"><image class="img" src="{{icon}}" mode="aspectFit"></image><view class="template-content"><view class="template-title"><search-highlight-text-view vue-id="{{'ad0108ae-2-'+index}}" highlight="highlight" text="{{item.name}}" keyword="{{keyName}}" onVueInit="__l"></search-highlight-text-view></view><view class="update-time">{{"模板更新时间："+item.gmtModified}}</view></view></view><block a:if="{{total<=contractTemplates.length&&total>0}}"><view class="touch-bottom--tips"><view class="line"></view><text class="words">已经到底了</text><view class="line"></view></view></block></view>