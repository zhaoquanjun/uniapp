<view class="add-person--page"><view class="title">输入信息</view><view class="value-list"><view class="list-item"><text class="sub-tit">公司</text><input type="text" placeholder="请输入" data-event-opts="{{[['input',[['handleInputCompanyNameFun',['$event']]]],['blur',[['handleBlurCompanyNameFun',['$event']]]]]}}" value="{{companyName}}" onInput="__e" onBlur="__e"/><view class="{{('suggest-list '+(companySuggests?'show':''))}}"><view class="item" data-item="{{item}}" data-event-opts="{{[['tap',[['handleSelectCompanySuggestFun',['$event']]]]]}}" catchTap="__e" a:for="{{companySuggests}}" a:for-item="item" a:for-index="index" a:key="index">{{item.name+" - "+item.phone}}</view></view></view><view class="list-item"><text class="sub-tit">经办人姓名</text><input type="text" placeholder="请输入" data-event-opts="{{[['input',[['handleInputUserNameFun',['$event']]]],['blur',[['handleBlurUserNameFun',['$event']]]]]}}" value="{{name}}" onInput="__e" onBlur="__e"/><view class="{{('suggest-list '+(suggests?'show':''))}}"><view class="item" data-item="{{item}}" data-event-opts="{{[['tap',[['handleSelectPersonSuggestFun',['$event']]]]]}}" catchTap="__e" a:for="{{suggests}}" a:for-item="item" a:for-index="index" a:key="index">{{item.name+" - "+item.phone}}</view></view></view><view class="list-item"><text class="sub-tit">经办人账号</text><input type="number" maxlength="11" placeholder="请输入" data-event-opts="{{[['input',[['handleInputUserPhoneFun',['$event']]]]]}}" value="{{phone}}" onInput="__e"/></view></view><view class="btn-group"><view data-event-opts="{{[['tap',[['handleBackFun',['$event']]]]]}}" class="cancel" onTap="__e">撤回</view><view data-event-opts="{{[['tap',[['handleConfirmFun',['$event']]]]]}}" class="{{('confirm '+(canNext?'canNext':''))}}" onTap="__e">确定</view></view></view>