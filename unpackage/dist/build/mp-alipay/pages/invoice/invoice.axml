<view class="invoice-list--page"><view class="search-area"><search vue-id="23b55bba-1" onTimeSearch="true" isShowCancel="true" data-event-opts="{{[['^inputChanged',[['inputSearchFun']]],['^cancelClicked',[['cancleSearchFun']]]]}}" onInputChanged="__e" onCancelClicked="__e" onVueInit="__l"></search></view><scroll-view class="invoice-list" scroll-y="true" refresher-enabled="true" lower-threshold="0" scroll-anchoring="true" refresher-triggered="{{isPullDown}}" data-event-opts="{{[['refresherrefresh',[['handlePullDownFun',['$event']]]],['scrollToLower',[['handleToBottomFun',['$event']]]]]}}" onRefresherrefresh="__e" onScrollToLower="__e"><view class="list-item" a:for="{{invoiceData}}" a:for-item="item" a:for-index="index" a:key="index"><view class="title"><view class="tit">{{item.contractSubjectTitle}}</view><view class="{{('status '+(item.status==0||item.status==1||item.status==6?'daishenpi':item.status==2?'yikaiju':item.status==3?'yijujue':item.status==4?'yibohui':''))}}">{{item.statusText}}</view></view><view class="desc"><view class="des">{{"发起方："+item.sellerCompanyName}}</view><block a:if="{{item.status==2||item.status==3}}"><view class="view-wrapper" data-item="{{item}}" data-event-opts="{{[['tap',[['handleShowSliderFun',['$event']]]]]}}" onTap="__e"><view class="more"></view></view></block></view><view class="date"><view>申请时间</view><view class="divider"></view><view>{{item.gmtCreate}}</view></view></view><view class="{{('space '+(isBottomRefresh?'refresh':''))}}"></view></scroll-view><slider-picker vue-id="23b55bba-2" sliderList="{{list}}" ref="__r" data-ref="sliderPicker" data-event-opts="{{[['^selectWay',[['handleSelectItemFun']]]]}}" onSelectWay="__e" onVueInit="__l"></slider-picker><message-box vue-id="23b55bba-3" messageInfo="{{messageInfo}}" ref="__r" data-ref="messageBox" onVueInit="__l"></message-box></view>