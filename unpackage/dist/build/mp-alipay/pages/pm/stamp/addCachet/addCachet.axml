<view><view class="input-view" data-index="{{index}}" a:for="{{dataMap}}" a:for-item="data" a:for-index="index" a:key="index"><text class="title">{{data.title}}</text><input class="input" data-index="{{index}}" placeholder="{{data.placeholder}}" auto-focus="{{true}}" data-event-opts="{{[['input',[['bindKeyInput',['$event']]]]]}}" onInput="__e"/></view><text class="stamp-title">印章颜色</text><view class="color-view"><view class="color" style="{{('border:'+(selectIndex==index?'3rpx '+colorData.rgb+' solid':''))}}" data-index="{{index}}" data-event-opts="{{[['tap',[['chooseColor',['$event']]]]]}}" catchTap="__e" a:for="{{colors}}" a:for-item="colorData" a:for-index="index" a:key="index"><view class="color-content" style="{{('background-color:'+colorData.rgb)}}"></view></view></view><button data-event-opts="{{[['tap',[['createCachet',['$event']]]]]}}" class="submit global-yellow-color" catchTap="__e">创建</button></view>