<view><view class="header-Bar"><view class="layout"><view data-event-opts="{{[['tap',[['chooseCompany',['$event']]]]]}}" class="currentName" catchTap="__e"><view class="_currentName">{{''+currentUser+''}}</view><image class="select" src="https://shouyiner-prod.oss-cn-beijing.aliyuncs.com/wxapp/shanqian/account/select_black.png"></image></view></view></view><view class="no-file" hidden="{{!isShowNoListView}}"><text class="no-file-title global-title-color">{{noStampTitle}}</text><text class="no-file-content global-content-color">{{noStampContent}}</text></view><view class="list-view"><view class="cell" data-item="{{stampData}}" data-index="{{index}}" data-event-opts="{{[['tap',[['handleShowOperateToolsFun',['$event']]]]]}}" onTap="__e" a:for="{{stampArray}}" a:for-item="stampData" a:for-index="index" a:key="index"><block a:if="{{stampData.isDefault==1}}"><view class="badge">默认</view></block><image class="cell-stamp-image" src="{{stampData.url}}" data-index="{{index}}" mode="aspectFit"></image></view><view style="width:100%;height:80rpx;"></view></view><block a:if="{{isShowAdd}}"><view data-event-opts="{{[['tap',[['addStampAction',['$event']]]]]}}" class="uploadButton" catchTap="__e"><text class="iconfont iconadd"></text></view></block><view class="{{('user-dialog '+(showDialog?'user-dialog--show':''))}}"><view data-event-opts="{{[['tap',[['toggleDialog',['$event']]]]]}}" class="user-dialog__mask" onTap="__e"></view><view class="user-dialog__container"><view class="userSwitch">切换身份</view><scroll-view style="height:540rpx;" scroll-y="{{true}}" nowrap="{{true}}"><view class="userList" data-item="{{item}}" data-index="{{index}}" data-event-opts="{{[['tap',[['switchUser',['$event']]]]]}}" onTap="__e" a:for="{{userList}}" a:for-item="item" a:for-index="index" a:key="index"><block a:if="{{item.authStatus}}"><image class="userList-icon" src="https://shouyiner-prod.oss-cn-beijing.aliyuncs.com/wxapp/shanqian/account/user.png"></image></block><block a:if="{{item.auth==0||item.auth==1}}"><image class="userList-icon" src="https://shouyiner-prod.oss-cn-beijing.aliyuncs.com/wxapp/shanqian/account/person.png"></image></block><view class="userList-name">{{''+(item.companyName?item.companyName:'')+item.name+''}}</view><block a:if="{{item.authStatus}}"><view class="userList-state" style="{{('background:'+(item.authStatus==1?'rgba(62,180,168,0.1)':item.authStatus==2?'#FDF0E5':'#F4F4F4')+'; color:'+(item.authStatus==1?'#3EB4A8':item.authStatus==2?'#ED6C00':'#919191'))}}">{{''+(item.authStatus=='1'?"已认证":item.authStatus==2?"认证中":"未认证")}}</view></block><block a:if="{{item.auth==0||item.auth==1}}"><view class="userList-state" style="{{('background:'+(item.auth==1?'rgba(62,180,168,0.1)':'#F4F4F4')+'; color:'+(item.auth==1?'#3EB4A8':'#919191'))}}">{{''+(item.auth=='1'?"已认证":"未认证")}}</view></block></view></scroll-view><view class="scroll-under"></view></view></view><slider-picker vue-id="2aa94646-1" sliderList="{{list}}" ref="__r" data-ref="sliderPicker" data-event-opts="{{[['^selectWay',[['handleSelectItemFun']]]]}}" onSelectWay="__e" onVueInit="__l"></slider-picker></view>